{
  "name": "mmm-yes-quite",
  "version": "0.0.7",
  "description": "Open Source web site for the Mmm, Yes, Quite podcast",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.json,.jsx",
    "fix": "eslint --fix . --ext .js,.json,.jsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preversion": "git checkout master && git merge --no-ff develop -m 'Pull changes for release bump'",
    "postversion": "git push && git push --tags && git checkout develop && git merge --no-ff master -m 'Sync Develop branch' && git push",
    "install": "ln -si ../../tools/pre-commit.sh .git/hooks/pre-commit",
    "prod": "npm run build && pm2 start process.json",
    "watch-server": "nodemon index.js --config nodemon.json",
    "watch-bundle": "webpack-dev-server --inline --hot --content-base client --config config/webpack.dev.js",
    "build": "webpack -p --config config/webpack.prod.js",
    "start": "concurrently \"npm run watch-server\" \"npm run watch-bundle\"",
    "dist": "./tools/upgrade.sh",
    "reload": "./tools/reload-server.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJoker29/mmm-yes-quite.git"
  },
  "author": "Dewitt Buckingham (http://zerodaedalus.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaJoker29/mmm-yes-quite/issues"
  },
  "homepage": "https://github.com/DaJoker29/mmm-yes-quite#readme",
  "dependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "body-parser": "1.15.2",
    "concurrently": "3.1.0",
    "connect-redis": "3.1.0",
    "debug": "https://registry.npmjs.org/debug/-/debug-2.4.5.tgz",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "express-session": "1.14.2",
    "helmet": "3.1.0",
    "isomorphic-fetch": "https://registry.npmjs.org/isomorphic-fetch/-/isomorphic-fetch-2.2.1.tgz",
    "moment": "https://registry.npmjs.org/moment/-/moment-2.17.1.tgz",
    "mongoose": "4.7.4",
    "mongoose-unique-validator": "1.0.3",
    "morgan": "1.7.0",
    "nodemon": "1.11.0",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth": "1.0.0",
    "rand-token": "0.3.0",
    "react": "https://registry.npmjs.org/react/-/react-15.4.1.tgz",
    "react-dom": "https://registry.npmjs.org/react-dom/-/react-dom-15.4.1.tgz",
    "react-hot-loader": "https://registry.npmjs.org/react-hot-loader/-/react-hot-loader-1.3.1.tgz",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.1.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "https://registry.npmjs.org/webpack-dev-server/-/webpack-dev-server-1.16.2.tgz",
    "webpack-hot-middleware": "https://registry.npmjs.org/webpack-hot-middleware/-/webpack-hot-middleware-2.13.2.tgz"
  }
}
